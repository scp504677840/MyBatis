<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.mapper.AccountInfoMapper">

    <!--
    resultType写集合中的类型
    -->
    <select id="mapAccountInfo" resultType="map">
        select *
        from account_info
        where id = #{id}
    </select>

    <!-- 查询多条记录时，resultType写集合中的类型 -->
    <select id="mapAccountInfos" resultType="main.entities.AccountInfo">
        select *
        from account_info
    </select>

</mapper>